{
  "name": "isomorphic-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "cross-env": "^5.1.1",
    "ejs-loader": "^0.3.0",
    "isomorphic-loader": "^1.6.4",
    "react": "^16.1.1",
    "react-document-meta": "^2.1.2",
    "react-dom": "^16.1.1",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^1.2.1",
    "serialize-javascript": "^1.4.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack --config webpack.dev.js --watch",
    "build": "cross-env NODE_ENV=production webpack --config webpack.prod.js -p",
    "prod": "cross-env-shell NODE_ENV=production npm run build && cross-env NODE_PATH=./src node server.js",
    "start-client": "cross-env NODE_ENV=development BUILD_TYPE=client webpack-dev-server --open --hot --config webpack.dev.js"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "eslint": "^4.12.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.5.1",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "ignore-styles": "^5.0.1",
    "jest": "^21.2.1",
    "node-sass": "^4.7.2",
    "nodemon": "^1.12.5",
    "react-test-renderer": "^16.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.1.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.1",
    "webpack-shell-plugin": "^0.5.0"
  }
}
